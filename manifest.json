{
	"manifest_version": 2,
	"name": "vbstudio-gitmoji",
	"version": "1.0",

	"description": "Adds an gitmoji picker on Oracle Visual Builder Studio commit summary inputs.",

	"icons": {
		"48": "icons/vbstudio-gitmoji-icon-48.png",
		"96": "icons/vbstudio-gitmoji-icon-96.png"
	},
	"content_scripts": [
		{
			"matches": ["*://*.ocp.oraclecloud.com/*"],
			"js": ["dist/index.js"],
			"css": ["dist/index.css"]
		}
	]
}
